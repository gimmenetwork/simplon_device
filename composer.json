{
  "name": "gimmenetwork/simplon_device",
  "description": "[FORK] Device detection based on piwik/device-detector",
  "homepage": "https://github.com/fightbulc/simplon_device",
  "type": "library",
  "keywords": [
    "device",
    "detection"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tino Ehrich",
      "email": "tino@bigpun.me",
      "role": "developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_interfaces"
    }
  ],
  "require": {
    "php": ">=7.1",
    "mobiledetect/mobiledetectlib": "^2.8",
    "jaybizzle/crawler-detect": "^1.2",
    "gimmenetwork/simplon_interfaces": "^0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "Simplon\\Device\\": "src/"
    }
  },
  "replace": {
    "simplon/device": "0.3.*"
  },
}
